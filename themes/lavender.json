{
    "name": "lavender",
    "type": "dark",
    "tokenColors": [
        {
            "settings": {
                "background": "#29222e",
                "foreground": "#e0ceed",
                "selectionForeground": "#ffffff",
                "findHighlight": "#ffe792",
                "findHighlightForeground": "#000000",
                "selectionBorder": "#222218",
                "activeGuide": "#9d550f",
                "bracketsForeground": "#a29dfa",
                "bracketsOptions": "underline",
                "bracketContentsForeground": "#b657ff",
                "bracketContentsOptions": "underline",
                "tagsOptions": "stippled_underline"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#614e6e"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#f5b0ef"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#f25ae6"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#b657ff"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#b657ff"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#8e6da6"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
