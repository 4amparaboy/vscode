{
    "name": "scorch-contrast",
    "type": "dark",
    "tokenColors": [
        {
            "settings": {
                "background": "#160d0e",
                "foreground": "#baa0a2",
                "selectionForeground": "#ffffff",
                "findHighlight": "#f7cc20",
                "findHighlightForeground": "#3d3206",
                "selectionBorder": "#ffffff",
                "activeGuide": "#331d20",
                "bracketsForeground": "#baa0a2",
                "bracketsOptions": "underline",
                "bracketContentsForeground": "#baa0a2",
                "bracketContentsOptions": "underline",
                "tagsOptions": "stippled_underline"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#44272a"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#a58163"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#cc8f5d"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#bd4549"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#f6ea82"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "",
                "foreground": "#d39452"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "",
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#cf9e51"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "",
                "foreground": "#e8d0d2"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#d39452"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#e77757"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "background": "#00a8c6",
                "foreground": "#f8f8f0",
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#00a8c6",
                "foreground": "#f8f8f0"
            }
        },
        {
            "name": "JSON String",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#a58163"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#44272a"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#e61f44"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#A6E22E"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#f7b83d"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#8FBE00A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#E6DB74"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#66383c"
            }
        },
        {
            "scope": [
                "meta.property-value",
                "support.constant.property-value",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#a58163"
            }
        },
        {
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#bd4549"
            }
        },
        {
            "scope": "meta.structure.dictionary.value.json string.quoted.double.json",
            "settings": {
                "foreground": "#a58163"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-name support.type.property-name",
            "settings": {
                "fontStyle": "normal"
            }
        },
        {
            "name": "Function argument",
            "scope": "meta.property-value punctuation.separator.key-value",
            "settings": {
                "foreground": "#baa0a2"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "keyword.other.use",
                "keyword.other.function.use",
                "keyword.other.namespace",
                "keyword.other.new",
                "keyword.other.special-method",
                "keyword.other.unit",
                "keyword.other.use-as"
            ],
            "settings": {
                "foreground": "#d39452"
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "meta.use support.class.builtin",
                "meta.other.inherited-class support.class.builtin"
            ],
            "settings": {
                "foreground": "#d39452",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.other",
            "settings": {
                "foreground": "#e8d0d2",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Coffeescript Function argument",
            "scope": "variable.parameter.function.coffee",
            "settings": {
                "foreground": "#cc8f5d",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown Titles",
            "scope": "entity.name.section.markdown",
            "settings": {
                "foreground": "#bd4549"
            }
        },
        {
            "name": "Markdown Title Hash",
            "scope": "punctuation.definition.heading.markdown",
            "settings": {
                "foreground": "#f6ea82"
            }
        },
        {
            "name": "Markdown Raw",
            "scope": "markup.raw.inline.markdown",
            "settings": {
                "foreground": "#a58163"
            }
        },
        {
            "name": "Markdown bold stars",
            "scope": [
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "foreground": "#f6ea82"
            }
        },
        {
            "name": "Markdown link title braces",
            "scope": [
                "punctuation.definition.string.begin.markdown",
                "punctuation.definition.string.end.markdown"
            ],
            "settings": {
                "foreground": "#f6ea82"
            }
        },
        {
            "name": "Markdown link braces",
            "scope": "punctuation.definition.metadata.markdown",
            "settings": {
                "foreground": "#f6ea82"
            }
        },
        {
            "name": "Markdown link",
            "scope": [
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
                "meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#d39452",
                "fontStyle": ""
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": [
                "markup.bold.markdown",
                "markup.italic.markdown"
            ],
            "settings": {
                "foreground": "#d39452"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown bold/italic",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown pre",
            "scope": "markup.raw.block.markdown",
            "settings": {
                "foreground": "#00a8c6"
            }
        },
        {
            "name": "GitGutter deleted",
            "scope": "markup.deleted.git_gutter",
            "settings": {
                "foreground": "#e61f44"
            }
        },
        {
            "name": "GitGutter inserted",
            "scope": "markup.inserted.git_gutter",
            "settings": {
                "foreground": "#a7da1e"
            }
        },
        {
            "name": "GitGutter changed",
            "scope": "markup.changed.git_gutter",
            "settings": {
                "foreground": "#f7b83d"
            }
        },
        {
            "name": "Template expressions.",
            "scope": "meta.template.expression",
            "settings": {
                "foreground": "#baa0a2"
            }
        }
    ],
